// Single Level Directory Simulation
#include <stdio.h>
#include <string.h>

struct file {
    char name[20];
} dir[10];

int main() {
    int n;
    printf("Enter number of files: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%s", dir[i].name);
    }

    char search[20];
    printf("Search file: ");
    scanf("%s", search);

    for (int i = 0; i < n; i++) {
        if (strcmp(dir[i].name, search) == 0) {
            printf("File found!\n");
            return 0;
        }
    }

    printf("File not found.\n");
    return 0;
}
